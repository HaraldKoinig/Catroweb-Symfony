{
    "name": "symfony/framework-standard-edition",
    "description": "The \"Symfony Standard Edition\" distribution",
    "type": "project",
    "license": [
        "MIT"
    ],
    "autoload": {
        "psr-0": {
            "": "src/",
            "SymfonyStandard": "app/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "require": {
        "php": ">=7.1",
        "symfony/symfony": "3.4.*",

        "symfony/swiftmailer-bundle": "^3.2",
        "symfony/webpack-encore-pack": "^1.0",
        "symfony/monolog-bundle": "^3.3",

        "friendsofsymfony/user-bundle": "^2.1",
        "friendsofsymfony/jsrouting-bundle": "^2.2",
        "sonata-project/user-bundle": "^4.2",
        "sonata-project/admin-bundle": "^3.37",
        "sonata-project/doctrine-orm-admin-bundle": "^3.6",
        "sonata-project/datagrid-bundle": "^2.3",
        "fr3d/ldap-bundle": "^3.0",

        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^5.2",
        "incenteev/composer-parameter-handler": "^2.1",

        "ext-json": "^1.5",
        "facebook/graph-sdk": "^5.6",
        "symfony/assetic-bundle": "^2.8",
        "google/apiclient": "^2.2",
        "willdurand/geocoder-bundle": "^5.1",
        "eightpoints/guzzle-bundle": "^7.3",

        "doctrine/doctrine-bundle": "^1.9",
        "doctrine/orm": "^2.6",
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "doctrine/data-fixtures": "^1.3",
        "doctrine/migrations": "^1.8",
        "doctrine/doctrine-migrations-bundle": "^1.3",

        "hwi/oauth-bundle": "*@dev",
        "knplabs/knp-paginator-bundle": "dev-master",
        "braincrafted/bootstrap-bundle": "dev-master",
        "twitter/bootstrap": "2.3.*",

        "ircmaxell/password-compat": "~1.0",
        "liip/theme-bundle": "dev-master",
        "twbs/bootstrap": "^3.3",

        "dmore/behat-chrome-extension": "^1.2",
        "symfony/polyfill-iconv": "^1.8",
        "ext-SimpleXML": "^7.1",
        "friendsofsymfony/rest-bundle": "^2.3"
    },
    "require-dev": {
        "behat/behat": "^3.5",
        "behat/symfony2-extension": "^2.1",
        "behat/mink-extension": "^2.3",
        "behat/mink-browserkit-driver": "^1.3",
        "behat/mink-goutte-driver": "^1.2",
        "behat/mink-selenium2-driver": "^1.3",
        "symfony/phpunit-bridge": "*",
        "phpunit/phpunit": "^7.3",
        "phpspec/phpspec": "^5.0",
        "sensio/generator-bundle": "^3.1",
        "sensiolabs/behat-page-object-extension": "^2.1",
        "vipsoft/doctrine-data-fixtures-extension": "3.0.*@dev"
    },
    "scripts": {
        "post-root-package-install": [
            "SymfonyStandard\\Composer::hookRootPackageInstall"
        ],
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-web-dir": "web",
        "symfony-app-dir": "app",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
